      }
    },
    "apply_material": {
      "type": "object",
      "required": ["type", "target"],
      "properties": {
        "type": {"const": "apply_material"},
        "target": {"type": "string"},
        "search_method": {"enum": ["by_name", "by_path", "by_tag"], "default": "by_name"},
        "color": {"$ref": "#/definitions/color"},
        "material_preset": {"type": "string"}
      }
    },
    "create_light": {
      "type": "object",
      "required": ["type", "light_type"],
