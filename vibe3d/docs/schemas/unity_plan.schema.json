        "type": {"const": "move_relative"},
        "target": {"type": "string"},
        "direction": {"enum": ["left", "right", "up", "down", "forward", "back"]},
        "distance": {"type": "number", "minimum": 0},
        "search_method": {"enum": ["by_name", "by_path", "by_id"], "default": "by_name"}
      }
    },
    "find_objects": {
      "type": "object",
      "required": ["type", "search_term"],
