{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Unity Action Plan",
  "description": "Validated action plan for Unity MCP execution â€” 63 action types covering all MCP tools",
  "type": "object",
  "required": ["project", "scene", "actions"],
  "properties": {
    "project": {
      "type": "string",
      "description": "Unity project name"
    },
    "scene": {
      "type": "string",
      "description": "Target scene name"
    },
    "description": {
      "type": "string",
      "description": "Human-readable description of the plan"
    },
    "actions": {
      "type": "array",
      "minItems": 1,
      "maxItems": 100,
      "items": {
        "$ref": "#/definitions/action"
      }
    }
  },
  "definitions": {
    "vec3": {
      "type": "object",
      "properties": {
        "x": {"type": "number"},
        "y": {"type": "number"},
        "z": {"type": "number"}
      },
      "required": ["x", "y", "z"]
    },
    "color": {
      "type": "object",
      "properties": {
        "r": {"type": "number", "minimum": 0, "maximum": 1},
        "g": {"type": "number", "minimum": 0, "maximum": 1},
        "b": {"type": "number", "minimum": 0, "maximum": 1},
        "a": {"type": "number", "minimum": 0, "maximum": 1, "default": 1}
      },
      "required": ["r", "g", "b"]
    },
    "action": {
      "type": "object",
      "required": ["type"],
      "oneOf": [
        {"$ref": "#/definitions/create_primitive"},
        {"$ref": "#/definitions/create_empty"},
        {"$ref": "#/definitions/modify_object"},
        {"$ref": "#/definitions/delete_object"},
        {"$ref": "#/definitions/apply_material"},
        {"$ref": "#/definitions/create_light"},
        {"$ref": "#/definitions/set_parent"},
        {"$ref": "#/definitions/duplicate_object"},
        {"$ref": "#/definitions/screenshot"},
        {"$ref": "#/definitions/save_scene"},
        {"$ref": "#/definitions/execute_menu"},
        {"$ref": "#/definitions/get_hierarchy"},
        {"$ref": "#/definitions/import_asset"},
        {"$ref": "#/definitions/add_component"},
        {"$ref": "#/definitions/set_component_property"},
        {"$ref": "#/definitions/create_material"},
        {"$ref": "#/definitions/assign_material"},
        {"$ref": "#/definitions/create_prefab"},
        {"$ref": "#/definitions/instantiate_prefab"},
        {"$ref": "#/definitions/create_particle_system"},
        {"$ref": "#/definitions/create_texture"},
        {"$ref": "#/definitions/move_relative"},
        {"$ref": "#/definitions/find_objects"},
        {"$ref": "#/definitions/add_tag"},
        {"$ref": "#/definitions/set_layer"},
        {"$ref": "#/definitions/editor_control"},
        {"$ref": "#/definitions/remove_component"},
        {"$ref": "#/definitions/set_material_color"},
        {"$ref": "#/definitions/set_material_property"},
        {"$ref": "#/definitions/get_material_info"},
        {"$ref": "#/definitions/modify_prefab"},
        {"$ref": "#/definitions/get_prefab_info"},
        {"$ref": "#/definitions/get_prefab_hierarchy"},
        {"$ref": "#/definitions/create_vfx"},
        {"$ref": "#/definitions/create_line_renderer"},
        {"$ref": "#/definitions/set_line_positions"},
        {"$ref": "#/definitions/create_trail_renderer"},
        {"$ref": "#/definitions/apply_texture_pattern"},
        {"$ref": "#/definitions/apply_texture_gradient"},
        {"$ref": "#/definitions/apply_texture_noise"},
        {"$ref": "#/definitions/create_sprite"},
        {"$ref": "#/definitions/create_scene"},
        {"$ref": "#/definitions/load_scene"},
        {"$ref": "#/definitions/get_active_scene"},
        {"$ref": "#/definitions/get_build_settings"},
        {"$ref": "#/definitions/remove_tag"},
        {"$ref": "#/definitions/add_layer"},
        {"$ref": "#/definitions/remove_layer"},
        {"$ref": "#/definitions/set_active_tool"},
        {"$ref": "#/definitions/search_assets"},
        {"$ref": "#/definitions/get_asset_info"},
